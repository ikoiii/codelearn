# Keystore configuration example
# Copy this file to keystore.properties and fill in your actual keystore information
# DO NOT commit keystore.properties to version control

# Release keystore configuration
keystore.file=../keystore/release.keystore
keystore.password=your_keystore_password
keystore.keyAlias=your_key_alias
keystore.keyPassword=your_key_password

# Debug keystore configuration (optional - Android Studio uses default debug keystore)
debug.keystore.file=../keystore/debug.keystore
debug.keystore.password=android
debug.keystore.keyAlias=androiddebugkey
debug.keystore.keyPassword=android